{
  "name": "jest-dynalite",
  "version": "3.2.4",
  "description": "Run your tests using Jest & Dynalite",
  "license": "MIT",
  "repository": "https://github.com/freshollie/jest-dynalite",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": {
    "name": "Oliver Bell",
    "email": "freshollie@gmail.com",
    "url": "https://obell.dev"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "yarn build && eslint --ext js,jsx,ts,tsx .",
    "test": "jest",
    "test:types": "tsc --noEmit",
    "e2e": "echo 'Testing simple preset'\n cd e2e/preset\n yarn\n yarn test\n echo 'Testing simple preset in monorepo'\n cd ../monorepo\n yarn\n yarn test",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepare": "yarn build"
  },
  "files": [
    "dist",
    "withDb.js",
    "environment.js",
    "clearAfterEach.js",
    "environment.js",
    "setupTables.js",
    "jest-preset.js"
  ],
  "keywords": [
    "jest",
    "dynamodb",
    "dynamodb local",
    "dynalite",
    "jest preset",
    "jest environment"
  ],
  "dependencies": {
    "dynalite": "^3.0.0"
  },
  "peerDependencies": {
    "aws-sdk": "2.x.x",
    "jest": ">=20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jest/types": "^26.6.2",
    "@types/cwd": "^0.10.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "aws-sdk": "^2.796.0",
    "babel-jest": "^26.6.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
